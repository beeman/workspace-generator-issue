{
  "version": 2,
  "projects": {
    "native": {
      "root": "apps/native",
      "sourceRoot": "apps/native/src",
      "projectType": "application",
      "generators": {},
      "targets": {
        "start": {
          "executor": "@nrwl/react-native:start",
          "options": {
            "port": 8081
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "command": "nx start native"
          }
        },
        "run-ios": {
          "executor": "@nrwl/react-native:run-ios",
          "options": {}
        },
        "bundle-ios": {
          "executor": "@nrwl/react-native:bundle",
          "outputs": [
            "apps/native/build"
          ],
          "options": {
            "entryFile": "apps/native/src/main.tsx",
            "platform": "ios",
            "bundleOutput": "dist/apps/native/ios/main.bundle"
          }
        },
        "run-android": {
          "executor": "@nrwl/react-native:run-android",
          "options": {}
        },
        "build-android": {
          "executor": "@nrwl/react-native:build-android",
          "outputs": [
            "apps/native/android/app/build/outputs/bundle",
            "apps/native/android/app/build/outputs/apk"
          ],
          "options": {}
        },
        "bundle-android": {
          "executor": "@nrwl/react-native:bundle",
          "options": {
            "entryFile": "apps/native/src/main.tsx",
            "platform": "android",
            "bundleOutput": "dist/apps/native/android/main.bundle"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/native/**/*.{js,ts,tsx}"
            ]
          }
        },
        "sync-deps": {
          "executor": "@nrwl/react-native:sync-deps",
          "options": {}
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/native/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "native-post": {
      "root": "libs/native/post",
      "sourceRoot": "libs/native/post/src",
      "projectType": "library",
      "generators": {},
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/native/post/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/native/post/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "defaultProject": "native"
}